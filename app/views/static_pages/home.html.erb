    <section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Skontaktuj się z nami</h2>
                    <h3 class="section-subheading text-muted">Wpisz swoje imie, email i kilka słów o tym, czego potrzebujesz.</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <%= form_for @contact_form, :url => submit_form_static_pages_path(@contact_form, :anchor => 'form') do |f| %>
                    <div class="row">
                      <%= render 'shared/error_messages' %>
                      
                      <div class="col-md-6">
                          
                        <div class="form-group">
                            
                          
                          <%= f.text_field :name, :class => 'form-control', :placeholder => "Twoje Imię *" %>
                          <p class="help-block text-danger"></p>
                        </div>
                        
                        <div class="form-group">
                          
                          <%= f.email_field :email, :class => 'form-control', :placeholder => "Twój Email *" %>
                          <p class="help-block text-danger"></p>
                        </div>
                    
                    
                        <div class="form-group">
                          
                          <%= f.text_field :phone, :class => 'form-control', :placeholder => "Twój Telefon" %>
                          <p class="help-block text-danger"></p>
                        </div>
                      </div>
                      
                      <div class="col-md-6">
                        <div class="form-group">
                          
                          <%= f.text_area :message, :class => 'form-control', :placeholder => "Twoja Wiadomość *" %>
                          <p class="help-block text-danger"></p>
                        </div>
                      </div>
                    <div class="clearfix"></div>
                    <div class="col-lg-12 text-center">
                        <div class="actions">
                          <%= f.submit "Send Message", :class => 'btn btn-xl'%>
                        </div>
                    </div>
                    </div>
                  <% end %>                 
                </div>
            </div>
        </div>
    </section>

